{
  "version": 1,
  "projects": {
    "demo": {
      "projectType": "application",
      "root": "apps/demo",
      "sourceRoot": "apps/demo/src",
      "prefix": "modules-demo-nx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/demo",
            "index": "apps/demo/src/index.html",
            "main": "apps/demo/src/main.ts",
            "polyfills": "apps/demo/src/polyfills.ts",
            "tsConfig": "apps/demo/tsconfig.app.json",
            "assets": ["apps/demo/src/favicon.ico", "apps/demo/src/assets"],
            "styles": ["apps/demo/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/demo/src/environments/environment.ts",
                  "with": "apps/demo/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": true,
              "outputHashing": "none",
              "namedChunks": true
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            },
            "development": {
              "browserTarget": "demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/demo/src/**/*.ts",
              "apps/demo/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/demo"],
          "options": {
            "jestConfig": "apps/demo/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "demo-e2e": {
      "root": "apps/demo-e2e",
      "sourceRoot": "apps/demo-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/demo-e2e/cypress.json",
            "devServerTarget": "demo:serve:development",
            "tsConfig": "apps/demo-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/demo-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "demo-lib": {
      "projectType": "library",
      "root": "libs/demo-lib",
      "sourceRoot": "libs/demo-lib/src",
      "prefix": "modules-demo-nx",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/demo-lib"],
          "options": {
            "project": "libs/demo-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/demo-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/demo-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/demo-lib"],
          "options": {
            "jestConfig": "libs/demo-lib/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/demo-lib/src/**/*.ts",
              "libs/demo-lib/src/**/*.html"
            ]
          }
        }
      }
    },
    "demo-lib-copy": {
      "projectType": "library",
      "root": "libs/demo-lib-copy",
      "sourceRoot": "libs/demo-lib-copy/src",
      "prefix": "modules-demo-nx",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/demo-lib-copy"],
          "options": {
            "jestConfig": "libs/demo-lib-copy/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/demo-lib-copy/src/**/*.ts",
              "libs/demo-lib-copy/src/**/*.html"
            ]
          }
        }
      }
    },
    "demo-lib2": {
      "projectType": "library",
      "root": "libs/demo-lib2",
      "sourceRoot": "libs/demo-lib2/src",
      "prefix": "modules-demo-nx",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/demo-lib2"],
          "options": {
            "project": "libs/demo-lib2/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/demo-lib2/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/demo-lib2/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/demo-lib2"],
          "options": {
            "jestConfig": "libs/demo-lib2/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/demo-lib2/src/**/*.ts",
              "libs/demo-lib2/src/**/*.html"
            ]
          }
        }
      }
    },
    "demo-node": {
      "root": "apps/demo-node",
      "sourceRoot": "apps/demo-node/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/demo-node",
            "main": "apps/demo-node/src/main.ts",
            "tsConfig": "apps/demo-node/tsconfig.app.json",
            "assets": [],
            "externalDependencies": "none"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": []
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "demo-node:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/demo-node/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/demo-node"],
          "options": {
            "jestConfig": "apps/demo-node/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "util-ng": {
      "projectType": "library",
      "root": "libs/util-ng",
      "sourceRoot": "libs/util-ng/src",
      "prefix": "modules-demo-nx",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/util-ng"],
          "options": {
            "project": "libs/util-ng/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/util-ng/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/util-ng/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/util-ng"],
          "options": {
            "jestConfig": "libs/util-ng/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/util-ng/src/**/*.ts",
              "libs/util-ng/src/**/*.html"
            ]
          }
        }
      }
    },
    "util-node": {
      "root": "libs/util-node",
      "sourceRoot": "libs/util-node/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/util-node",
            "tsConfig": "libs/util-node/tsconfig.lib.json",
            "packageJson": "libs/util-node/package.json",
            "main": "libs/util-node/src/index.ts",
            "assets": ["libs/util-node/*.md"]
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/util-node/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/util-node"],
          "options": {
            "jestConfig": "libs/util-node/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "defaultProject": "demo"
}
